<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@+id/background">
        <bitmap android:src="@drawable/star_filled_yellow" android:tint="@color/ratingNormal" android:alpha="?android:attr/disabledAlpha"/>
    </item>
    <item android:id="@+id/secondaryProgress">
        <bitmap android:src="@drawable/star_filled_yellow" android:tint="?attr/colorControlActivated"/>
    </item>
    <item android:id="@+id/progress">
        <bitmap android:src="@drawable/star_filled_yellow" android:tint="?attr/colorControlActivated" android:tileModeX="repeat"/>
    </item>
</selector>
